{
	"name": "fivem-launcher",
	"version": "1.0.0",
	"description": "electron application",
	"main": "main.js",
	"scripts": {
		"dev": "webpack-dev-server --config ./webpack.config.js",
		"publish": "set NODE_ENV=production&& webpack --config ./webpack.config.js -p",
		"l:publish": "NODE_ENV=production webpack --config ./webpack.config.js -p",
		"run_electron": "set NODE_ENV=dev && electron ./main",
		"l:run_electron": "NODE_ENV=dev electron ./main",
		"preview": "npm run publish && set NODE_ENV=dev && electron ./main",
		"electron:rebuild": "npm rebuild --runtime=electron --target=3.0.7 --disturl=https://atom.io/download/atom-shell --abi=64",
		"pack": "npm run publish && set NODE_ENV=production&& electron-builder --win --x64 portable",
		"linux:pack": "npm run l:publish && NODE_ENV=production electron-builder --linux --x64 portable"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Aktyn/fivem-launcher.git"
	},
	"keywords": [
		"fivem",
		"launcher",
		"electron",
		"webpack"
	],
	"author": "Aktyn",
	"license": "UNLICENSED",
	"bugs": {
		"url": "https://github.com/Aktyn/fivem-launcher/issues"
	},
	"homepage": "https://github.com/Aktyn/fivem-launcher#readme",
	"dependencies": {
		"@types/electron-json-storage": "^4.5.0",
		"@types/node": "^18.14.6",
		"@types/node-fetch": "^2.6.2",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"node-fetch": "^3.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^6.8.2",
		"react-router-dom": "^6.8.2"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.13",
		"awesome-typescript-loader": "^5.2.1",
		"css-loader": "^6.7.3",
		"electron": "^23.1.3",
		"electron-builder": "^23.6.0",
		"electron-rebuild": "^3.2.9",
		"file-loader": "^6.2.0",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.0",
		"image-webpack-loader": "^8.1.0",
		"mini-css-extract-plugin": "^2.7.3",
		"node-sass": "^8.0.0",
		"postcss-loader": "^7.0.2",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"typescript": "^4.9.5",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1"
	},
	"build": {
		"appId": "fivem.launcher",
		"copyright": "Copyright Â© 2019 Aktyn",
		"win": {
			"target": "portable",
			"icon": "icon.png"
		},
		"directories": {
			"output": "electron_dist"
		}
	},
	"browser": {
		"fs": false
	}
}
